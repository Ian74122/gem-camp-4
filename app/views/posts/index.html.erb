<h1>Post List</h1>
<%= link_to 'New', new_post_path %>

<table>
  <thead>
  <tr>
    <td>title</td>
    <td>content</td>
    <td>action</td>
  </tr>
  </thead>
  <% @posts.each do |post| %>
    <tr>
      <td><%= post.title %></td>
      <td><%= post.content %></td>
      <td>
        <%= link_to :show, post_path(post) %>
      </td>
    </tr>
  <% end %>
</table>

